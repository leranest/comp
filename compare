def lev(str1, str2):
    n = len(str1)
    m = len(str2)

    def f(n, m):
        if n == 0 or m == 0: #если какая-то из двух строк пустая
            return max(n, m) #возвращаем длину не пустой строки (ну или 0, если обе пустые)
        elif str1[n - 1] == str2[m - 1]: #если в двух сравниваемых строках последние элементы равны:
            return f(n - 1, m - 1) #возвращаем f без последних элементов
        else:
            # в другом случае мы считаем минимальный вариант при удалении символа, встаки символа или замены символа
            return (min(f(n, m - 1), f(n - 1, m), f(n - 1, m - 1)) + 1)
    return f(len(str1), len(str2))

file1= #в переменную заносим в виде строки программу из файла
file2= #в эту пееменную заносим в виде строки программу, которцю будем сравнивать с первой

file1=file1.replace(" ", "") #удаляем все пробелы из строки1
file2=file2.replace(" ", "") #удаляем все пробелы из строки2

file1=file1.lower() #приводим строку 1 к строчным символам
file2=file2.lower() #приводим строку 2 к строчным символам

levinshtein=lev(file1,file2)
ratio=((max([len(file1), len(file2)])-levinshtein)/ max([len(file1), len(file2)])) #находим соотношение (процент) схожести двух файлов
